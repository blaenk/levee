{
  "name": "torrent-levee",
  "version": "0.0.1",
  "description": "rtorrent user interface",
  "main": "./lib/index",
  "files": [
    "lib/"
  ],
  "scripts": {
    "clean": "rm -rfv ./lib/",
    "build": "babel src/node --out-dir lib --source-maps",
    "watch": "babel src/node --out-dir lib --source-maps --watch",
    "start": "npm run build && node ./lib/app.js",
    "test": "npm run build && mocha --compilers js:babel-core/register test/node",
    "test-watch": "npm run build && mocha --compilers js:babel-core/register --recursive --watch test/node",
    "compile": "babel src/node --out-dir lib --ignore src/node/app.js",
    "prepublish": "npm run compile",
    "grunt": "node Gruntfile.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blaenk/levee.git"
  },
  "keywords": [
    "rtorrent"
  ],
  "author": "Jorge Israel Pe√±a",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/blaenk/levee/issues"
  },
  "homepage": "https://github.com/blaenk/levee",
  "devDependencies": {
    "grunt": "~0.4.5",
    "grunt-contrib-less": "~1.2.0",
    "grunt-contrib-watch": "~0.6.1",

    "babel": "^6.3.26",
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "chai": "^3.4.1",
    "chai-as-promised": "^5.2.0",
    "mocha": "^2.3.4",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0",
    "source-map-support": "^0.4.0"
  },
  "dependencies": {
    "bluebird": "^3.1.1",
    "body-parser": "^1.14.2",
    "dotenv": "^2.0.0",
    "express": "^4.13.3",
    "lodash": "^4.4.0",
    "moment": "^2.10.6",
    "request": "^2.67.0",
    "request-promise": "^2.0.0"
  }
}
